<div class= 'container'>
  <div class = 'page-header'>
    <%= link_to 'New Project', new_project_path, class: "btn btn-info pull-right" %>
    <h1>Projects</h1>
  </div>


  <table class = "table">
    <thead>
      <tr>
        <th> Name </th>
      </tr>
    </thead>
    <% @projects.each do |project| %>
      <tbody>
        <tr>
          <td><%= link_to project.name, project_path(project) %></td>
        </tr>
      </tbody>
    <% end %>
  </table>


</div>

